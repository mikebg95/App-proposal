<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mike.eattrainreap">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MyWorkoutsActivity" />
        <activity android:name=".GoalsProgressActivity" />
        <activity android:name=".ExercisesActivity" />
        <activity android:name=".WorkoutScheduleActivity" />
        <activity android:name=".PreviousWorkoutsActivity" />
        <activity android:name=".WorkoutActivity" />
        <activity android:name=".FindExerciseActivity" />
        <activity android:name=".PreviousExercisesActivity" />
        <activity android:name=".SavedExercisesActivity" />
        <activity
            android:name=".PopUpActivity"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity android:name=".EditInfoActivity" />
        <activity android:name=".NewWorkoutActivity" />
        <activity android:name=".CommentActivity" />
        <activity android:name=".WorkoutInfoActivity" />
        <activity android:name=".EditWorkoutActivity"></activity>
    </application>

</manifest>